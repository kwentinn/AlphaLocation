@using AgicapAppCore.Cqrs.Queries.RecuPaiements.Administration.GetPaymentReceiptById
@using AgicapAppCore.Pdf.Templates.Data

@inherits RazorLight.TemplatePage<LocalizedModel<PaymentReceipt>>
@{
    PaymentReceipt paymentReceipt = Model.Model;
}
<div class="row">
    <div class="col-xs-6">
        <img class="logo" src=" data:image/png;base64," />
    </div>
    <div class="col-xs-6">
        <div class="boite-info-facture-haut-droite">
            <div class="row">
                <div class="col-xs-12">
                    <p class="title-box">
                        @if (paymentReceipt.IsAvoir)
                        {
                            <span>@Model.GetTranslation("Templates.PaymentReceipt.AVOIR") </span>
                        }
                        else
                        {
                            <span>@Model.GetTranslation("Templates.PaymentReceipt.FACTURE") </span>
                        }
                    </p>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-12">
                    <p class="info-facture">
                        @if (paymentReceipt.IsAvoir)
                        {
                            <span class="left">@Model.GetTranslation("Templates.PaymentReceipt.AVOIR") </span>
                        }
                        else
                        {
                            <span class="left">@Model.GetTranslation("Templates.PaymentReceipt.FACTURE") </span>
                        }
                        <span>n° @paymentReceipt.Id</span>
                    </p>
                </div>

            </div>
            <div class="row">
                <div class="col-xs-12">
                    <p class="info-facture">
                        @*Date d'émission*@
                        @Model.GetTranslation("Templates.PaymentReceipt.CreationDate")
                        <span>@PaymentReceipt.FormatDate(paymentReceipt.DateEmission)</span>
                    </p>
                </div>
            </div>
        </div>

    </div>
</div>
